#include<iostream>
#include<vector>
using namespace std;

int find(vector<int>&arr){
    for(int i=0; i<arr.size(); i++){
        for(int j=i+1; j<arr.size(); j++){
            if(arr[i]==arr[j]){
                cout<<"find : "<<i+1;
              
            }
        }
    }
    return -1;
   
}

int main(){
  vector<int>arr={1,8,2,3,2,6};
  find(arr);
} 
