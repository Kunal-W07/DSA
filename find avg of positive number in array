#include <iostream>
#include <vector>
using namespace std;

int main() {
    // Write C++ code here
     vector<int>arr ={-3,2,0,-7,25,12,-11};
     int count = 0;
     float sum = 0;
     
     for(int i=0; i<arr.size(); i++){
         if(arr[i] > 0){
             sum += arr[i];
             count++;
         }
     }
     float avg = sum /2;
     cout<<"the positiive avg is "<<avg;
    

    return 0;
}
