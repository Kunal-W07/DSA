class Solution {
  public:
    string toLower(string& s) {
        
        int n = s.length();
        
        for(int i=0; i<n; i++){
            if(s[i] >= 'A' && s[i] <= 'Z'){
                char ch = s[i];
                ch = ch - 'A' + 'a';
                s[i] = ch;
            }
        }
        return s;
    }
};
